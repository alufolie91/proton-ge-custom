+    THISCALLMETHOD_(HRESULT, BeginUAVOverlap)(THIS) PURE;
+    THISCALLMETHOD_(HRESULT, EndUAVOverlap)(THIS) PURE;
+    THISCALLMETHOD_(void, GetVersion)(THIS, AmdDxExtVersion* version) PURE;
