@@ -156,6 +158,7 @@ static void InitFunctionPtrs(void)
     ok(hntdll != 0, "LoadLibrary failed\n");
     if (hntdll) {
         pRtlDelete = (void *)GetProcAddress(hntdll, "RtlDelete");
